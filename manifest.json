{
  "manifest_version": 3,
  "name": "DeepSeek Chat",
  "version": "1.0",
  "description": "Chat with DeepSeek AI directly from your browser",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": [
    "https://ai.hackclub.com/*"
  ]
}
